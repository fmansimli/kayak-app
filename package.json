{
  "name": "kayak-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "clean-build:win": "cd android && gradlew cleanBuildCache && gradlew clean && cd ..",
    "clean-build:linux": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
    "generate-apk:win": "npm run clean-build:win && npx react-native run-android --variant=release",
    "generate-apk:linux": "npm run clean-build:linux && npx react-native run-android --variant=release",
    "generate-aab:win": "npm run clean-build:win && cd android && gradlew bundleRelease && cd ..",
    "generate-aab:linux": "npm run clean-build:linux && cd android && ./gradlew bundleRelease && cd ..",
    "generate-keystore": "cd android/app && keytool -genkeypair -v -storetype PKCS12 -keystore upload-key.keystore -alias upload-alias -keyalg RSA -keysize 2048 -validity 10000",
    "remove-keystore": "cd android/app && rm upload-key.keystore && cd ../../",
    "import-cert": "cd android/app && keytool.exe -importcert -file upload_cert.der -keystore upload-key.keystore",
    "show-debug-certs":"cd android/app && keytool -keystore debug.keystore -list -v",
    "show-upload-certs":"cd android/app && keytool -keystore uplaod-key.keystore -list -v",
    "prod-ios": "appcenter codepush release-react -a faridmansimli/kayak-ios -d Production",
    "dev-ios": "appcenter codepush release-react -a faridmansimli/kayak-ios -d Staging",
    "prod-android": "appcenter codepush release-react -a faridmansimli/kayak-android -d Production",
    "dev-android": "appcenter codepush release-react -a faridmansimli/kayak-android -d Staging",
    "dev-all": "npm run dev-android && npm run dev-ios",
    "prod-all": "npm run prod-android && npm run prod-ios",
    "push-all": "npm run dev-all && npm run prod-all",
    "build": "npm install && cd ./android && ./gradlew build && cd ../ios && pod install --repo-update && cd ..",
    "format": "prettier --write \"**/*.+(js|ts|tsx)\"",
    "clean": "rm -rf ./node_modules **/{.project,*.iml,build} ./ios/{Pods,*.xcworkspace} ./android/{.gradle,.idea}",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "expo-modules": "npx install-expo-modules",
    "show-android-keys": "appcenter codepush deployment list -a faridmansimli/kayak-android -k",
    "show-ios-keys": "appcenter codepush deployment list -a faridmansimli/kayak-ios -k"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-native-firebase/app": "^14.2.2",
    "@react-native-firebase/auth": "^14.2.2",
    "@react-native-google-signin/google-signin": "^7.0.4",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "appcenter": "4.3.0",
    "appcenter-analytics": "4.3.0",
    "appcenter-crashes": "4.3.0",
    "expo": "~44.0.2",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "formik": "^2.2.9",
    "react": "17.0.1",
    "react-native": "0.64.3",
    "react-native-code-push": "^7.0.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^1.13.3",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-native": "^0.66.11",
    "@types/react-test-renderer": "^17.0.1",
    "typescript": "^4.5.4"
  },
  "private": true
}
